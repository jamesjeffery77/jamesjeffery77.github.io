{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Carbon per capita by country",

"title": {
    "text": "Electricity Makuep",
    "subtitle":"How electricity contributes to everything",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },

  "data": {
    "url": "https://raw.githubusercontent.com/jamesjeffery77/jamesjeffery77.github.io/main/test.csv"
  },
  "height": 300,
  "width": 300,
  "mark": "line",
  "encoding": {
    "x": {"field": "Year", "type": "temporal", "title": "", "axis": {"grid": false}},
    "y": {
      "field": "%",
      "type": "quantitative",
      "title": "",
      "axis": {"grid": false},
      "sort": []
    },
    "color": {
      "field": "Type",
      "title": null,
      "legend": {"orient": "top-left"},
      "type": "nominal",
      "sort": [],
      "scale": {"scheme": "yellowgreenblue"}
    },
    "tooltip": [
      {"field": "Year", "type": "temporal", "timeUnit": "year", "title": "Year"},
      {"field": "%", "type": "quantitative", "title": "% which contributes"},
      {"field": "Type"}
    ]
  }
}
